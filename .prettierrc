{
	"trailingComma": "all",
	"tabWidth": 4,
	"useTabs": true,
	"semi": true,
	"singleQuote": true,
	"arrowParens": "always",
	"jsxSingleQuote": false,
	"jsxBracketSameLine": false,
	"bracketSpacing": true,
	"printWidth": 80,
	"endOfLine": "lf",
	"plugins": ["prettier-plugin-tailwindcss", "@prettier/plugin-ruby"]
}
